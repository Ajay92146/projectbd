    "setup": [
      "1. Install Node.js and MongoDB",
      "2. Clone the repository",
      "3. Run 'npm install' to install dependencies",
      "4. Set up environment variables in .env file",
      "5. Start the server with 'npm start'",
      "6. Access the application at http://localhost:3000"
    ],
    "testing": [
      "1. Use the sample data provided above",
      "2. Test donor registration with the sample donor data",
      "3. Test blood request submission with sample request data",
      "4. Test user authentication with sample user credentials",
      "5. Test search functionality with different blood groups and locations",
      "6. Verify all forms are working correctly",
      "7. Check responsive design on different screen sizes"
    ],
    "api_testing": [
      "1. Use Postman or similar tool to test API endpoints",
      "2. Test POST /api/donors with sample donor data",
      "3. Test GET /api/donors/search with blood group and location",
      "4. Test POST /api/requests with sample request data",
      "5. Test POST /api/auth/login with sample user credentials",
      "6. Test POST /api/auth/register with new user data",
      "7. Verify all API responses and error handling"
    ]
  },
